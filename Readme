function rangeOfNumbers(start,end) {
  let rangeArray = [];
  for (let i = start; i <= end; i++) {
    rangeArray.push(i + "");
  }
  return rangeArray.map(x => x.includes("2") ? "Boop!" : x.includes("1") ? "Beep" : x);

}
console.log(rangeOfNumbers(0,12).join());
Describe: Boop Beep
Test: "It should change any number with 2 to "BOOP" and any number with a 1 to "BEEP"
It works


function rangeOfNumbersFour(start,end) {
  let rangeArray = [];
  for (let i = start; i <= end; i++) {
    rangeArray.push(i + "");
  }
  return rangeArray.map(x => x.includes("3") ? "Wont you be my Neighbor?" : x.includes("21") ? "Boop" : x);

}
console.log(rangeOfNumbers(0,12).join());
Describe: Boop Beep
Test: "It should change any number with 3 to "Won't you be my Neighbor?" and the number 21 to "Boop"
It works


